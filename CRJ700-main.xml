			<enable-tiller type="bool">true</enable-tiller>
			<tiller-steer-deg type="float">0</tiller-steer-deg>
		</gear>
		<hydraulic>
			<system>
				<!-- 0 = off, 1 = on, 2 = auto -->
				<pump-a type="bool">true</pump-a>
				<pump-b type="int">0</pump-b>
				<hyd-sov-cover type="bool">false</hyd-sov-cover>
			</system>
			<system>
				<pump-a type="bool">true</pump-a>
				<pump-b type="int">0</pump-b>
				<hyd-sov-cover type="bool">false</hyd-sov-cover>
